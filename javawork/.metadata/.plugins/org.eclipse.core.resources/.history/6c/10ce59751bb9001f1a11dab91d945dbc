package day1211;

public class Ex1DataType {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		//byte에 숫자 넣어보기
		byte a=127;
		byte b=(byte)128;//(byte): 강제 형변환 -cast연산자
		System.out.println("a="+a);//127
		System.out.println("b="+b);//-128 :틀린값이 나온다, 범위를 벗어나므로
		System.out.println();

	}

}
